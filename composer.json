    "psr/http-message": "1.1",
    "laravel/octane": "1.5.5",
    "guzzlehttp/guzzle": "7.5.1",
    "google/apiclient": "2.13.2",
    "masbug/flysystem-google-drive-ext": "1.3.2",
    "php-open-source-saver/jwt-auth": "2.1.0"
