  "type": "library",
  "require": {
    "arcanedev/log-viewer": "8.1.0",
    "darkaonline/l5-swagger": "8.5",
    "doctrine/dbal": "2.13.9",
    "larapack/doctrine-support": "0.1.9",
    "laravel/sanctum": "2.15.1",
    "laravel/tinker": "2.8.1",
    "laravel/ui": "3.4.6",
